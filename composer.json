{
    "name": "glorand/laravel-model-settings",
    "description": "Model Settings for your Laravel app",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorand Gombos",
            "email": "gombos.lorand@gmail.com"
        }
    ],
    "type": "laravel-package",
    "keywords": [
        "glorand",
        "laravel-model-settings",
        "laravel-settings",
        "laravel",
        "settings"
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Glorand\\Model\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Glorand\\Model\\Settings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Glorand\\Model\\Settings\\ModelSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
